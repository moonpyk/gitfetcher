{
    "defaults": {
        "git_bin": "/opt/local/bin/git",
        "_git_bin_on_windows": "C:/Program Files/Git/cmd/git.exe",
        "_git_bin_on_windows_x64": "C:/Program Files (x86)/Git/cmd/git.exe",
        "pull": true,
        "fetch_all": true,
        "pull_ff_only": true,
        "print_git_out": true
    },
    "smuxi": {
        "path": "~/Code/CSharp/smuxi",
        "pull": false,
        "context": "cs"
    },
    "bootstrap": {
        "path": "~/Code/CSS/bootstrap",
        "context": [
            "css",
            "less",
            "js"
        ]
    }
}
